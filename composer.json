{
    "name": "guirong/php-closure",
    "description": "realize the serialization and deserialization of closures.",
    "keywords": ["closure", "serialization", "function", "serializable", "serialize", "anonymous functions"],
    "homepage": "https://github.com/GuiRong2969/php-closure",
    "license": "MIT",
    "authors": [
        {
            "name": "Rong Gui",
            "email": "15168272969@163.com"
        }
    ],
    "require": {
        "php": "^5.4 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "jeremeamia/superclosure": "^2.0",
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Guirong\\PhpClosure\\": "src/"
        },
        "files": ["functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Guirong\\PhpClosure\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.6.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
